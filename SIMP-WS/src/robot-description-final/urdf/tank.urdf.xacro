<?xml version="1.0"?>
<robot name="rrl_tank" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link">
    <inertial>
      <origin
        xyz="-0.0397613304580986 -0.00187447663786819 -0.0007976966428697"/>
      <mass value="10"/>
      <inertia
        ixx="0.280033582927332"
        ixy="-0.0109458890651764"
        ixz="-0.000139336718483203"
        iyy="0.441269583836676"
        iyz="9.2785508828341E-05"
        izz="0.185559829444659" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://atr_rrl_description/meshes/tank/base_link.STL"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.7 0.2"/>
        <!-- <box size="0.4 0.7 0.14"/> -->
        <!-- <mesh filename="package://atr_rrl_description/meshes/tank/base_link.STL"/> -->
      </geometry> 
    </collision>
  </link>
  
  <link name="right_front_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="-${pi / 2} 0 0"/>
      <mass value="0.168011919570301" />
      <inertia
        ixx="0.000126799359132551"
        ixy="4.2180094748375E-11"
        ixz="-9.55801614785873E-11"
        iyy="0.000112956774131014"
        iyz="2.87100617773173E-09"
        izz="0.000112951688012628" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://atr_rrl_description/meshes/tank/r_front_gear_link.STL"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0" />
      <geometry>
        <cylinder length="0.05207" radius="0.052" />
      </geometry>
    </collision>
  </link>

  <joint name="base_to_right_front_wheel" type="continuous">
    <origin xyz="0 -0.30 0.05"/>
    <axis xyz="0 1 0"/>
    <!-- <joint_properties damping="0.7"/> -->
    <limit effort="2000" velocity="100" />
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
  </joint>

  <link name="left_front_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="-${pi / 2} 0 0"/>
      <mass value="0.168011919570301" />
      <inertia
        ixx="0.000126799359132551"
        ixy="5.10665558282939E-11"
        ixz="9.11932344797647E-11"
        iyy="0.000112956782528061"
        iyz="-2.875306538123E-09"
        izz="0.000112951677864508" />
    </inertial>
    <visual>
      <origin rpy="${pi} 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://atr_rrl_description/meshes/tank/l_front_gear_link.STL"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0" />
      <geometry>
        <cylinder length="0.05207" radius="0.052" />
      </geometry>
    </collision>
  </link>

  <joint name="base_to_left_front_wheel" type="continuous">
    <origin xyz="0 0.30 0.05"/>
    <axis xyz="0 1 0"/>
    <!-- <joint_properties damping="0.7"/> -->
    <limit effort="2000" velocity="100" />
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
  </joint>

  <link name="right_rear_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="-${pi / 2} 0 0"/>
      <mass value="0.172189485487845" />
      <inertia
        ixx="0.000122934028959951"
        ixy="-9.69198780790459E-11"
        ixz="8.40716510084883E-11"
        iyy="0.000135229835441607"
        iyz="-3.35713405092348E-10"
        izz="0.000122926656061839" />
    </inertial>
    <visual>
      <origin rpy="-${pi / 2} 0 0" xyz="0 0.038 0"/>
      <geometry>
        <mesh filename="package://atr_rrl_description/meshes/tank/r_rear_gear_link.STL"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0" />
      <geometry>
        <cylinder length="0.05207" radius="0.052" />
      </geometry>
    </collision>
  </link>

  <joint name="base_to_right_rear_wheel" type="continuous">
    <origin xyz="-0.30 -0.30 0.05"/>
    <axis xyz="0 1 0"/>
    <!-- <joint_properties damping="0.7"/> -->
    <limit effort="2000" velocity="100" />
    <parent link="base_link"/>
    <child link="right_rear_wheel"/>
  </joint>

  <link name="left_rear_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="-${pi / 2} 0 0"/>
      <mass value="0.172189485243552" />
      <inertia
        ixx="0.000122934030023779"
        ixy="4.12976146803433E-10"
        ixz="-8.00350733438054E-10"
        iyy="0.000135229835425849"
        iyz="8.73155133668469E-12"
        izz="0.00012292611083972" />
    </inertial>
    <visual>
      <origin rpy="-${pi / 2} 0 0" xyz="0 -0.034 0"/>
      <geometry>
        <mesh filename="package://atr_rrl_description/meshes/tank/l_rear_gear_link.STL"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0" />
      <geometry>
        <cylinder length="0.05207" radius="0.052" />
      </geometry>
    </collision>
  </link>

  <joint name="base_to_left_rear_wheel" type="continuous">
    <origin xyz="-0.30 0.30 0.05"/>
    <axis xyz="0 1 0"/>
    <!-- <joint_properties damping="0.7"/> -->
    <limit effort="2000" velocity="100" />
    <parent link="base_link"/>
    <child link="left_rear_wheel"/>
  </joint>

  <gazebo>
    <!-- <selfCollide>false</selfCollide> -->
    <plugin name="skid_steer_drive_controller" filename="libgazebo_ros_skid_steer_drive.so">
      <rosDebugLevel>na</rosDebugLevel>
      <updateRate>100.0</updateRate>
      <leftFrontJoint>base_to_left_front_wheel</leftFrontJoint>
      <rightFrontJoint>base_to_right_front_wheel</rightFrontJoint>
      <leftRearJoint>base_to_left_rear_wheel</leftRearJoint>
      <rightRearJoint>base_to_right_rear_wheel</rightRearJoint>
      <wheelSeparation>0.5207</wheelSeparation>
      <wheelDiameter>0.0762</wheelDiameter>
      <robotBaseFrame>base_link</robotBaseFrame>
      <torque>500</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <broadcastTF>true</broadcastTF>
    </plugin>
  </gazebo>

</robot>